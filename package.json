{
  "name": "atree-react",
  "description": "A simple component for rendering an animated tree view",
  "version": "0.4.11",
  "type": "module",
  "private": false,
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist/ && tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/atree.es.js",
      "require": "./dist/atree.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/atree.umd.js",
  "module": "./dist/atree.es.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "peerDependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-react": "^2.2.0",
    "eslint": "^8.28.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "prettier": "^2.8.0",
    "react-hooks": "^1.0.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.7.1",
    "vite-tsconfig-paths": "^3.6.0"
  },
  "keywords": [
    "react",
    "tree",
    "view",
    "component",
    "ui",
    "animated",
    "tree-view",
    "atree"
  ],
  "author": {
    "email": "post@slytech.no",
    "name": "Martin Ottersland",
    "url": "https://github.com/SlyOtis"
  },
  "homepage": "https://github.com/SlyOtis/atree#readme",
  "repository": {
    "url": "https://github.com/SlyOtis/atree",
    "type": "git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlyOtis/atree/issues"
  }
}
